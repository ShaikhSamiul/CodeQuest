{
    "version": 2,
    
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "installCommand": "bash install.sh"
            }
        }
    ],
    
    "routes": [
        { "src": "/.*", "dest": "app.py" }
    ]
    
}
