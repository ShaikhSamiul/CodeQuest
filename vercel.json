{
    "version": 2,
    
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.11",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        { "src": "/.*", "dest": "app.py" }
    ]
    
}
