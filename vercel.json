{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "pip install -r requirements.txt -c constraints.txt"
            }
        }
    ],
    "routes": [
        { "src": "/.*", "dest": "app.py" }
    ]
}
