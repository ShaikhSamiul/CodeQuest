{
    "version":2,
    "builds":[
        {"src":"app.py", "use":"@vercel/python"}
    ],
    "routes":[
        {"src":"/.*","dest":"app.py"}
    ],
        "functions": {
          "api/*.js": {
            "includeFiles": ["dist/**"]
          }
        },
        "ignoreFiles": [
          "node_modules/**",
          ".git/**",
          "tests/**",
          "*.md"
        ],
      "dependencies": {
    "flask": "^3.0.3",
    "pymysql": "^1.1.1",
    "sqlalchemy": "^2.0.35"
  }
      
}